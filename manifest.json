{
  "name": "JS Runner",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Run js in tabs",
  "browser_action": {
    "default_icon": "img/icon.jpg",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": ["js/background.js"] ,
    "persistent": false
  },
  "permissions": ["storage", "tabs", "<all_urls>"]
}